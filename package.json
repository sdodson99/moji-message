{
  "name": "moji-message",
  "version": "0.1.0",
  "description": "Make a message out of emojis.",
  "author": "Sean Dodson",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint:eslint": "eslint ./src --ext=.tsx,.ts,.html",
    "lint:prettier": "prettier --check ./src",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint --fix ./src --ext=.tsx,.ts,.html",
    "fix:prettier": "prettier --write ./src",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci",
    "test:update": "jest --updateSnapshot",
    "e2e": "playwright test",
    "e2e:update": "npm run e2e -- --update-snapshots",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only hosting",
    "deploy:stage": "firebase hosting:channel:deploy --expires 7d",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@picmo/popup-picker": "^5.7.2",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.2",
    "next": "13.2.4",
    "next-seo": "^5.15.0",
    "picmo": "^5.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@types/jest": "^29.2.2",
    "@types/jest-when": "^3.5.2",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.13",
    "eslint": "8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "firebase-tools": "^11.16.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-when": "^3.5.2",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^29.0.3"
  }
}
